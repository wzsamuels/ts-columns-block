{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ts-columns/ts-columns",
	"version": "0.1.0",
	"title": "TS Columns",
	"category": "design",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
  "attributes": {
    "stack": {
      "type": "boolean",
      "default": true
    },
    "reverseStack" : {
      "type": "boolean",
      "default": false      
    },
    "columns": {
      "type": "number",
      "default": 1
    }
  },
	"textdomain": "ts-columns",
	"editorScript": "file:./index.js",
	"editorStyle": "file:../build/index.css",
  "style": ["file:../build/index.css"],
	"viewScript": "file:./view.js"
}
